{
    "name": "randomstate/laravel-api",
    "description": "Integration of randomstate/api with Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Connor Forsyth",
            "email": "connor@randomstate.co.uk"
        }
    ],
    "license": "MIT",
    "require": {
        "php" : ">=8.0",
        "randomstate/api": "^0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/laravel": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "RandomState\\LaravelApi\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RandomState\\Tests\\LaravelApi\\": "tests",
            "Tests\\": "vendor/laravel/laravel/tests"
        }
    }
}
